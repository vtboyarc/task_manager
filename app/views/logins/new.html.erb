<h1>Log In</h1>

<%= form_tag logins_path do %>
  <div class="field">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  
  <div class="field">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  
  <%= submit_tag "Log In" %>
<% end %>

<p><%= link_to 'Go Home', root_path %></p>